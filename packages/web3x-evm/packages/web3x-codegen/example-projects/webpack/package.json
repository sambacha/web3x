{
  "name": "web3.ts-webpack-example",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "web3x-codegen && webpack --config webpack.prod.js",
    "start": "node ./server",
    "start:dev": "webpack-dev-server --config webpack.dev.js",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "koa": "^2.5.2",
    "koa-static": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@types/jest": "^23.3.1",
    "@types/koa": "^2.0.46",
    "@types/koa-static": "^4.0.0",
    "@types/webpack-env": "^1.13.6",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "ts-jest": "^23.1.4",
    "ts-loader": "^4.4.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^2.9.1",
    "web3x-es": "^3.0.1",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  }
}
